            <column name="CUSTOMER_ID"/>
        </createIndex>
    </changeSet>
    <changeSet author="Rajashree Ravi" id="1698744855620-4">
		<preConditions onFail="MARK_RAN">
			<not>
				<foreignKeyConstraintExists foreignKeyName="FKLGKGAGXGMINJ5CC8SIUGP47AF" foreignKeyTableName="CUSTOMER_GRANTED_AUTHORITIES_LIST" />
			</not>
		</preConditions>
        <addForeignKeyConstraint baseColumnNames="CUSTOMER_ID" baseTableName="CUSTOMER_GRANTED_AUTHORITIES_LIST" constraintName="FKLGKGAGXGMINJ5CC8SIUGP47AF" deferrable="false" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="RESTRICT" referencedColumnNames="ID" referencedTableName="CUSTOMER" validate="true"/>
