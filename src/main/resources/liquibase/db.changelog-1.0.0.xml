		</preConditions>
        <createTable tableName="CUSTOMER_GRANTED_AUTHORITIES_LIST">
            <column name="CUSTOMER_ID" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="GRANTED_AUTHORITIES_LIST" type="VARBINARY(255)"/>
        </createTable>
    </changeSet>
    <changeSet author="Rajashree Ravi" id="1698744855620-3">
		<preConditions onFail="MARK_RAN">
